@model List<productModel>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Serial Num</th>
            <th>Item Name</th>
            <th>Barcode</th>
            <th>Brand ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.serialNum</td>
                <td>@item.itemName</td>
                <td>@item.barcode</td>
                <td>@item.brandID</td>
            </tr>
        }
    </tbody>
</table>

